<script>
$( document ).ready(function() {
	
	 $.ajax({url: "lennox/getdetailsfrominflux",
		 type: 'GET',
		// data:{"testname":$("#test_name").val(),"startTime":test_start,"endTime":test_end},
		 success: function(result){
	      //  $("#div1").html(result);
	      var res=JSON.parse(result);
	     // alert("res"+res.project);
	     console.log("response:",res);
	     $("#project").empty();
	     $("#project").append("<option value='0'>Choose Project,Scenario name</option>");
	     for(var i=0;i<res.length;i++)
	    	 {
	    	 var obj= res[i];
	    	 //alert(obj);
	    	 console.log("obj",obj);
	    	// console.log("value:",obj.project);
	    	
	    	 $("#project").append("<option value='"+obj.project+"|"+obj.suite+"'>"
	    			 +obj.project+","+obj.suite+"</option>");
	    	 }
	     // alert("Status:"+res);
	    },
	    error:function(error)
	    {
	    	}
		 });
	
$('#save').click(function() {
	  //  var name = $('iframe[name=select_frame]').contents().find('ng-modal=ctrl.timeRaw.from').val();
	//	alert("click");
		
	/* var test_start=Date.parse($("#testStart_time").val()+'+05:30');
	var test_end=Date.parse($("#testEnd_time").val()+'+05:00'); */
	var projectScenario=$( "#project" ).val();
	var arr=projectScenario.split("|");
	if(arr.length==0)
		{
		alert("please select project/scenario");
		return;
		}
	var project=arr[0];
	var scenario=arr[1]
	var test_start=Date.parse($("#testStart_time").val()+'');
	var test_end=Date.parse($("#testEnd_time").val()+'');
	 $.ajax({url: "lennox/savetestdetails",
		 type: 'POST',
		 data:{"project":project,"scenario":scenario,"testname":$("#test_name").val(),"startTime":test_start,"endTime":test_end},
		 success: function(result){
	      //  $("#div1").html(result);
	      var res=JSON.parse(result);
	      alert("Status:"+res.result);
	      
	    },
	    error:function(error)
	    {
	    	}
		 });
	});
});
</script>