<div ng-controller="smTestCoverageController as stcctrl">
	<div class="smTCPanel panel">
		<div class="panel-heading do-panel-heading">
			<div class="widgetMenuIncludeDiv"
				ng-include="'views/app/widgets/widgetMenu/scrum/widgetScrumTCMenu.html'"></div>
			<div class="panel-title">
				Test Coverage<br>
				<div class="panel_subTitle">{{sTCTitle}}</div>
			</div>
		</div>
		<div class="testCoveragePanel-body panel-body">

			<div class="row graphWell bTop">
				<div id="testCoverageLoadImg_id" class="loadImage_class">
					<i class="fa fa-spinner fa-spin fa-3x"></i>
				</div>
				<div ng-show="chartFlag">
					<div id="smTCChart" class="smTC"
						style="width: 600px; height: 300px;"></div>
				</div>
				<div ng-show="tableFlag">
					<div>
						<table class="table table-bordered">
							<thead>
								<tr>
									<th>Application Name</th>
									<th>Coverage</th>
								</tr>
							</thead>
							<tbody>
								<tr ng-repeat="(key,value) in totalMap">
									<td>{{key}}</td>
									<td>{{value}}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>