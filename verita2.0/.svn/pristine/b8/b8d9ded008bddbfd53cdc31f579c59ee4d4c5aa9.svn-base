<div ng-controller="lennoxComparisionKPIController as tdctrl">
	<div class="lennoxObservationsPanel panel"  style= "height: 378px;">
		<div class="panel-heading do-panel-heading">
			<div class="widgetMenuIncludeDiv"
				ng-include="'views/app/widgets/lennox/WidgetLennoxmenu/widgetLennoxObserForComparision.html'"></div>

			<div class="panel-title">KPI</div>
<!-- 			<div class="panel-title">Key Performance Indicators</div> -->
		</div>
		<div class="panel-body">
			<table class="table table-bordered">

				<thead>
					<tr>
						<th style="border-bottom: none;">&nbsp;</th>
						<th style="border-bottom: none;">&nbsp;</th>
						<th colspan="2" align="center" class="ng-binding">Actual</th>
						<th colspan="2" align="center" class="ng-binding">Diff</th>
						<th colspan="2" align="center" class="ng-binding">Diff %</th>
					</tr>
					<tr>

						<th>Metric</th>
						<th>Target</th>

						<th>Test With</th>
						<th>Test To</th>
						<th>Test With</th>
						<th>Test To</th>
						<th>Test With</th>
						<th>Test To</th>
					</tr>




				</thead>
				<tbody>
					<tr>
						<td>TPS</td>
						<td>{{tpsKPI}}</td>
						<td>{{tpsValueForTest1}}</td>
						<td>{{tpsValueForTest2}}</td>
						<td><span class="label  {{tpsColorTest1}}">{{tpsDiffForTest1}}</span></td>
						<td><span class="label  {{tpsColorTest2}}">{{tpsDiffForTest2}}</span></td>
						<td>{{tpsDiffPercentageForTest1}}</td>
						<td>{{tpsDiffPercentageForTest2}}</td>
					</tr>
					<tr>
						<td>Errors</td>
						<td>{{errorKPI}}</td>
						<td>{{errorValueForTest1}}</td>
						<td>{{errorValueForTest2}}</td>
						<td><span class="label  {{errorColorTest1}}">{{errorDiffForTest1}}</span></td>
						<td><span class="label  {{errorColorTest2}}">{{errorDiffForTest2}}</span></td>
						<td>{{errorDiffPercentageForTest1}}</td>
						<td>{{errorDiffPercentageForTest2}}</td>
					</tr>
					<tr>
						<td>Memory</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
					</tr>
					<tr>
						<td>CPU</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
						<td>--</td>
					</tr>

				</tbody>
			</table>
