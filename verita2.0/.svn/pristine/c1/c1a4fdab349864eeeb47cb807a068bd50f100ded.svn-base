package cgt.dop.alm.util;

import java.util.List;
import java.util.Map;

public class OpenDefectSeverityTO {
    
    private Map<String, DefectSeverityTO> openDefectSeverityMap;
    
    private List<DefectsTO> openDefects;
    
    private Map<String, DefectSeverityTO> allOpenDefects;
    
    private float allDefectSeverityCount;
    
    private float meanTotal;

    public Map<String, DefectSeverityTO> getOpenDefectSeverityMap() {
        return openDefectSeverityMap;
    }

    public void setOpenDefectSeverityMap(Map<String, DefectSeverityTO> openDefectSeverityMap) {
        this.openDefectSeverityMap = openDefectSeverityMap;
    }

    public List<DefectsTO> getOpenDefects() {
        return openDefects;
    }

    public void setOpenDefects(List<DefectsTO> openDefects) {
        this.openDefects = openDefects;
    }

    public Map<String, DefectSeverityTO> getAllOpenDefects() {
        return allOpenDefects;
    }

    public void setAllOpenDefects(Map<String, DefectSeverityTO> allOpenDefects) {
        this.allOpenDefects = allOpenDefects;
    }

    public float getMeanTotal() {
        return meanTotal;
    }

    public void setMeanTotal(float meanTotal) {
        this.meanTotal = meanTotal;
    }

    public float getAllDefectSeverityCount() {
        return allDefectSeverityCount;
    }

    public void setAllDefectSeverityCount(float allDefectSeverityCount) {
        this.allDefectSeverityCount = allDefectSeverityCount;
    }

}
