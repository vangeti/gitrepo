<div ng-controller="LeadTimeController as ltctrl">
	<div class="leadPanel panel">
		<div class="panel-heading do-panel-heading">
			<div class="widgetMenuIncludeDiv"
				ng-include="'views/app/widgets/widgetMenu/widgetCIOMenu.html'"></div>
			<div class="panel-title">
				Lead Time<br>
				<div class="panel_subTitle">{{leadTimeTitle}}</div>
			</div>
		</div>
		<div class="leadPanel-body panel-body">

			<div class="row graphWell bTop" align="center">
				<div ng-show="leadChartFlag">
					<div id="leadtime" class="lead"
						style="width: 600px; height: 300px;"></div>
				</div>
				<div ng-show="leadTableFlag">
					<table class="table table-bordered">
						<thead>
							<tr>
								<th>Sprint</th>
								<th>Week</th>
								<th>Reaction Time(days)</th>
								<th>Cycle Time(days)</th>

							</tr>
						</thead>
						<tbody>
							<tr>
								<td rowspan="2">Sprint1</td>
								<td>23/05/2016</td>
								<td>11</td>
								<td>17</td>
							</tr>
							<tr>
								<td>30/05/2016</td>
								<td>12</td>
								<td>23</td>
							</tr>
							<tr>
								<td rowspan="2">Sprint2</td>
								<td>06/06/2016</td>
								<td>12</td>
								<td>31</td>
							</tr>
							<tr>
								<td>13/06/2016</td>
								<td>13</td>
								<td>33</td>
							</tr>

							<tr>
								<td rowspan="2">Sprint3</td>
								<td>20/06/2016</td>
								<td>12</td>
								<td>32</td>
							</tr>
							<tr>
								<td>27/06/2016</td>
								<td>16</td>
								<td>39</td>
							</tr>

							<tr>
								<td rowspan="2">Sprint4</td>
								<td>04/07/2016</td>
								<td>12</td>
								<td>29</td>
							</tr>
							<tr>
								<td>11/07/2016</td>
								<td>14</td>
								<td>36</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>