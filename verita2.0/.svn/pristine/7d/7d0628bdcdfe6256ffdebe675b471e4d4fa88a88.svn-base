<div ng-controller="SeleniumTestDetailsController as stdc">
	<div class="stdcPanel panel">
		<div class="panel-heading do-panel-heading">
			<div class="widgetMenuIncludeDiv"
				ng-include="'views/app/widgets/widgetMenu/widgetPerfMenu.html'"></div>
			<div class="panel-title">
				Test Execution Summary<br>
				<div class="panel_subTitle">{{date}}</div>
			</div>
		</div>
		<div class="stdcPanel-body panel-body">

			<div class="row graphWell">
				<div id="coderepo1" class="stdcTestDetailsTable">

					<div class="row bTop">
						<div class="col-md-4 mTop">
							<div class="tdTopTitle">
								Project Name<br>
								<div class="tdTop_subTitle">Konakart</div>
							</div>
						</div>
						<div class="col-md-4 mTop">
							<div class="tdTopTitle">
								Duration<br>
								<div class="tdTop_subTitle1">{{duration}}</div>
							</div>
						</div>
						<div class="col-md-4 mTop">
							<div class="tdTopTitle">
								Build No<br>
								<div class="tdTop_subTitle1">{{buildNo}}</div>
							</div>
						</div>
					</div>
					<div class="row bTop">
						<div class="col-md-6 mTop" style="border-right: 1px solid #ccc;">
							<div class="row tdBottom_Title">Total Test Details</div>
							<div class="row">
								<div class="col-md-6 tdBotTitle">
									Tests<br>
									<div class="tdBot_subTitle">{{totalTestsRun}}</div>
								</div>
								<div class="col-md-6 tdTopTitle">
									Success<br>
									<div class="tdBot_subTitle">{{succesFullTests}}</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-6 tdTopTitle">
									Failure<br>
									<div class="tdBot_subTitle">{{failureTests}}</div>
								</div>
								<div class="col-md-6 tdTopTitle">
									Skips<br>
									<div class="tdBot_subTitle">{{skipTests}}</div>
								</div>
							</div>
						</div>
						<div class="col-md-6 mTop">
							<div class="row tdBottom_Title">Coverage</div>
							<div class="row">
								<div class="col-md-6 tdTopTitle">
									Browser<br>
									<div class="tdBot_subTitle">{{browserName}}</div>
								</div>
								<div class="col-md-6 tdTopTitle">
									Device/Desktop<br>
									<div class="tdBot_subTitle">Desktop</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-6 tdTopTitle">
									OS<br>
									<div class="tdBot_subTitle">{{os}}</div>
								</div>
								<div class="col-md-6 tdTopTitle">
									Provider<br>
									<div class="tdBot_subTitle">OnPrem</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="modal fade" id="seleniumReport" role="dialog">
			<div class="modal-dialog modal-lg">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal">&times;</button>
						<h4 class="modal-title">Konakart - Detailed Test Report</h4>
					</div>
					<div class="modal-body" style="min-height: 500px; height: 500px;">
						<div class="row">
							<iframe
								src="http://172.16.3.42:8080/jenkins/view/Demo%20View/job/Demo_Selenium%20Unit%20Test"
								style="width: 100%; height: 500px;"></iframe>
						</div>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>