<div ng-controller="perfJMKPIImpController as jmkctrl">
	<div class="jmkpiPanel panel">
		<div class="panel-heading do-panel-heading">
			<!-- <div class="widgetMenuIncludeDiv"
				ng-include="'views/app/widgets/widgetMenu/widgetPerfMenu.html'"></div> -->
			<div class="panel-title">
				KPI Status Since Last Build<br>
			</div>
		</div>
		<div class="panel-body">

			<div id="perfJMKPILoadImg_id" class="loadImage_class">
				<i class="fa fa-spinner fa-spin fa-3x"></i>
			</div>

			<div class="row graphWell" align="center">
				<div class="kpiPanelContent">
					<div class="row bTop">
						<div class="col-md-10 tdTopTitle" align="left">
							Status<br>
							<div class="tdTop_subTitle">Improvement Status</div>
						</div>
						<div class="col-md-2" style="color: red; margin-top: 20px;">
							<i class="fa fa-circle" aria-hidden="true"></i>
						</div>
					</div>

					<table class="table kpiTable">
						<thead>
							<tr>
								<th>Metric</th>
								<th>KPI(Sec)</th>
								<th>Prev</th>
								<th>Curr</th>
								<th>Diff</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>Avg. Resp Time</td>
								<td>1.2</td>
								<td>{{PrevrespTime}}</td>
								<td>{{respTime}}</td>
								<td class="{{respcolor}} diffcss">{{respTimeDiff}}</td>
							</tr>
							<tr>
								<td>TPS</td>
								<td>0.20</td>
								<td>{{Prevtps}}</td>
								<td>{{tps}}</td>
								<td class="{{tpscolor}} diffcss">{{tpsTimeDiff}}</td>
							</tr>
							<tr>
								<td>Errors</td>
								<td>10.0</td>
								<td>{{Preverrors}}</td>
								<td>{{errors}}</td>
								<td class="{{errorcolor}} diffcss">{{errorTimeDiff}}</td>
							</tr>

						</tbody>
					</table>

				</div>
			</div>
		</div>
		<!-- <div class="panel-footer" align="right">
			<span class="widgetFooterSpan" data-toggle="tooltip"
				data-placement="bottom" title="Export Excel"><i
				class="fa fa-file-excel-o" aria-hidden="true"></i></span>
		</div> -->
	</div>
</div>