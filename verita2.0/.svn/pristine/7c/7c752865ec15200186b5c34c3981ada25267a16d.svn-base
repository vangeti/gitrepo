<div ng-controller="smTestCaseYieldController as stcctrl">
	<div class="smTCYPanel panel">
		<div class="panel-heading do-panel-heading">
			<div class="widgetMenuIncludeDiv"
				ng-include="'views/app/widgets/widgetMenu/widgetCIOMenu.html'"></div>
			<div class="panel-title">
				Test Case Yield<br>
				<div class="panel_subTitle">{{sTCYTitle}}</div>
			</div>
		</div>
		<div class="testYieldPanel-body panel-body">

			<div class="row graphWell bTop">
				<div id="testCaseYieldLoadImg_id" class="loadImage_class">
					<i class="fa fa-spinner fa-spin fa-3x"></i>
				</div>
				<div ng-show="chartFlag">
					<div id="smTCYChart" class="smTCY"
						style="width: 600px; height: 300px;"></div>
				</div>
				<div ng-show="tableFlag">
					<div>
						<table class="table table-bordered">
							<thead>
								<tr>
									<th>Application Name</th>
									<th>Test Case Yield</th>
								</tr>
							</thead>
							<tbody>
								<tr ng-repeat="(key,value) in tcAllDefects">
									<td>{{key}}</td>
									<td>{{value}}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>